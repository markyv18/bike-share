<h2 class='title'><%= @station.name %></h2>

<p class='city'>City: <%= @station.city %></p>
<p class='dock_count'>Dock Count: <%= @station.dock_count %></p>
<p class='installation_date'>Installation Date: <%= @station.installation_date %></p>

<form action="/stations/<%= @station.id %>" method="POST">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete"/>
</form>
